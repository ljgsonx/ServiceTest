<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ljgsonx.servicetest" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.ljgsonx.servicetest.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="com.example.ljgsonx.servicetest.MyService" />
        <service android:name="com.example.ljgsonx.servicetest.MyService2" />
        <service android:name="com.example.ljgsonx.servicetest.MyIntentService" />
    </application>

</manifest>
